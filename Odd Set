#include<bits/stdc++.h>
using namespace std;

int main(){
	int t;
	cin>>t;
	while(t--){
		int n,odd=0,even=0;
		cin>>n;
		int arr[2*n];
		for(int i=0;i<2*n;i++){
            cin>>arr[i];
            if(arr[i]&1){
            	odd++;
            }
            else{
            	even++;
            }
		}
		if(even==odd){
			cout<<"YES\n";
		}
		else{
			cout<<"NO\n";
		}

	}
	return 0;
}
